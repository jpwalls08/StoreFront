@model IEnumerable<StoreFront.DATA.EF.Models.EquipmentType>

@{
    ViewData["Title"] = "Shop Equipment";
}

@*<h1>Index</h1>*@

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EquipmentTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EquipmentTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EquipmentTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
